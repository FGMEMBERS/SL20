	<!-- Gas toggles -->
	<animation>
		<type>translate</type>
		<object-name>gas_toggle.1</object-name>
		<property alias="../../params/gas-valve-cmd-norm[0]"/>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>-0.15</dep>
			</entry>
		</interpolation>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>gas_toggle.1</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property alias="../../../../params/gas-valve-cmd-norm[0]"/>
				<value type="double">0.0</value>
				<value type="double">1.0</value>
			</binding>
			<binding>
