<?xml version="1.0"?>

<!-- altimeter -->
<PropertyList> 
	
	<path>asimeter.ac</path>
	
	<offsets>
		<!-- x/y/z == back/right/up -->
		<x-m> 0.000 </x-m>
		<y-m> 0.000 </y-m>
		<z-m> 0.000 </z-m>
	</offsets>
	
	<!-- Rembrandt transparency markup. -->
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>glass.001</object-name>
		<object-name>card2</object-name>
	</effect>
	
	<animation> 
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>/velocities/airspeed-kt</property>
		<interpolation>
			<entry><ind>     0 </ind><dep>   0 </dep></entry>
			<entry><ind> 64.79 </ind><dep> 336 </dep></entry>
		</interpolation>
		<axis>
			<x1-m> -0.00679 </x1-m>
			<y1-m>  0.00    </y1-m>
			<z1-m>  0.15997 </z1-m>
			<x2-m>  0.00741 </x2-m>
			<y2-m>  0.00    </y2-m>
			<z2-m>  0.14589 </z2-m>
		</axis>
	</animation>
	<animation> 
		<type>material</type>
		<object-name>card2</object-name>
		<emission> 
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>
	
</PropertyList>
