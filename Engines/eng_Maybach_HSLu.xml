<piston_engine name="Maybach HSLu">
	
	<displacement unit="LTR"> 19.08  </displacement>
	<bore unit="M">            0.150 </bore>
	<stroke unit="M">          0.180 </stroke>
	<compression-ratio>        5.50  </compression-ratio>
	<cylinders>                6.00  </cylinders>
	<cycles>                   4.00  </cycles>
	
	<minmp unit="INHG">         10.5 </minmp>
	<maxmp unit="INHG">         28.5 </maxmp>
	
	<maxhp>                   240.00 </maxhp> <!-- Needs to be adjusted so that
	full throttle at 1800 m
	delivers 240 hp @ 1400 RPM(?).
	-->
	
	<idlerpm>                  90.00 </idlerpm>
	<maxrpm>                 1400.00 </maxrpm>
	<starter-rpm>             400.00 </starter-rpm>
	<bsfc unit="LBS/HP*HR">     0.5146611248 </bsfc>
	<volumetric-efficiency>     0.85 </volumetric-efficiency>
	<sparkfaildrop>             0.10 </sparkfaildrop>
	
	<minthrottle> 0.1 </minthrottle>
	<maxthrottle> 1.0 </maxthrottle>
	
</piston_engine>
