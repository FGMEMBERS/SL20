  <crank>
   <name>crank 1</name>
   <path>Sounds/coughing.wav</path>
   <mode>looped</mode>
   <condition>
    <property>engines/engine/cranking</property>
    <not>
     <property>engines/engine/running</property>
    </not>
   </condition>
   <position>
    <!-- x/y/z == back/left/up -->
    <x>  60.0 </x>
    <y>   6.5 </y>
    <z> -10.0 </z>
   </position>
   <reference-dist>10.0</reference-dist>
   <volume>
    <offset>0.4</offset>
   </volume>
  </crank>
